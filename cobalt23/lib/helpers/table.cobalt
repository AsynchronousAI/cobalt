var TableHelper = {};

function TableHelper.pack(...) {
  if( table.pack != null ) {
    return table.pack(...);
  }

  return { n = select("#", ...), ... };
}

function TableHelper.unpack(...) {
  if( table.unpack != null ) {
    return table.unpack(...);
  }

  return unpack(...);
}

return TableHelper;
