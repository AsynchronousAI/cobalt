// add line numbers to code output
var string = require ('ext.string');
var function showcode(code) {
	var lines = string.split(string.trim(code),'\n');
	var max = tostring(#lines);
	return lines->map(function(l,i) {
		var num = tostring(i);
		return num..':'..(' ')->rep(#max - #num)..l;
	})->concat('\n');
}
return showcode;
